<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.SampleTagMapper">
    <select id="selectSampleTag" resultType="sampleTag">
        SELECT *
        FROM SAMPLE_TAG
    </select>

    <insert id="insertSampleTag" parameterType="sampleTag">
        INSERT INTO SAMPLE_TAG (ID, NAME) VALUES (#{id}, #{name})
    </insert>
</mapper>
